/*
	binop test
*/

// globals
int a = 0;
int b = 2;

float c = 0.5;
float d = 4.5;

bool e = true;
bool f = false;

export void main ()
{
	a = 1 * 2; // INT + INT
	b = 3 - a; // INT + varINT
	c = 3.4 / c; // FLOAT + varFLOAT
	a = a + b; // varINT + varINT
	c = c >= d; // varFLOAT + varFLOAT

// MAG NIET:
	a = a < d; // varINT + varFLOAT
	b = b > c; // INT + FLOAT
	c = c <= e; // FLOAT + BOOL
	a = a % c; // INT + FLOAT
	f = e + true; // varBOOL + BOOL

// Alles hierboven werkt.
}